  "scripts": {
    "dev": "tsx server/index.ts",
    "dev:unix": ". ./env && NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "node dist/index.js",
    "check": "tsc",
    "db:validate": "tsx scripts/validate-db-schema.ts",
    "smoke:xrpl-swap-v2": "tsx scripts/smoke/xrpl-swap-v2-smoke.ts",
    "smoke:xrpl-liquidity-limit": "tsx scripts/smoke/xrpl-liquidity-limit-smoke.ts",
    "db:push": "drizzle-kit push",
    "test:gcs": "./test-gcs.sh",
    "setup:gcs": "./setup-gcs.sh",
    "verify-security": "node verify-endpoints.js",
    "alias:scan": "tsx -r tsconfig-paths/register run-scanner.ts once",
    "alias:node": "node -r ts-node/register -r tsconfig-paths/register",
    "alias:tsx": "tsx -r tsconfig-paths/register",
    "scan:complete": "tsx -r tsconfig-paths/register complete-nft-scanner.ts",
    "scan:fast": "tsx -r tsconfig-paths/register fast-nft-scanner.ts",
    "scan:test:dantes": "tsx -r tsconfig-paths/register test-dantes-scanner.ts",
    "scan:players": "tsx --env-file=.env -r tsconfig-paths/register player-scoring-scanner.ts",
    "verify:db:players": "tsx --env-file=.env -r tsconfig-paths/register verify-player-scoring-tables.ts",
    "fix-security": "node fix-security.js",
    "scan:nfts": "tsx run-scanner.ts once",
    "scan:nfts:scheduled": "tsx run-scanner.ts scheduled",
    "cap:sync": "npx cap sync",
    "cap:android": "npm run build && npx cap sync android && npx cap open android",
    "cap:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "build:android": "cp .env.android .env.local && npm run build && npx cap sync android",
    "build:ios": "cp .env.ios .env.local && npm run build && npx cap sync ios",
    "build:windows": "cp .env.windows .env.local && npm run build",
    "deploy:android": "npm run build:android && cd android && ./gradlew assembleRelease",
    "deploy:ios": "npm run build:ios && cd ios/App && xcodebuild -workspace App.xcworkspace -scheme App -configuration Release archive",
    "deploy:windows": "npm run build:windows && npm run start"
  },
